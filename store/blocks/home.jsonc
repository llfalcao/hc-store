{
    "store.home": {
        "blocks": ["tab-layout#home"]
    },
    "flex-layout.row": {
        "props": {
            "fullWidth": true
        },
        "children": ["flex-layout.col"]
    },
    "flex-layout.col": {
        "props": {
            "width": "100%"
        },
        "children": ["tab-layout#home"]
    },
    "tab-layout#home": {
        "children": [
            "tab-list#home",
            "slider-layout#images",
            "tab-content#home"
        ]
    },
    "tab-list#home": {
        "props": {
            "blockClass": "home",
            "defaultActiveTabId": "home-1"
        },
        "children": [
            "tab-list.item#home-1",
            "tab-list.item#home-2",
            "tab-list.item#home-3",
            "tab-list.item#home-4",
            "tab-list.item#home-5",
            "tab-list.item#home-6",
            "tab-list.item#home-7",
            "tab-list.item#home-8",
            "tab-list.item#home-9",
            "tab-list.item#home-10"
        ]
    },
    "tab-list.item#home-1": {
        "props": {
            "tabId": "home-1",
            "label": "Promoção",
            "defaultActiveTab": true
        }
    },
    "tab-list.item#home-2": {
        "props": {
            "tabId": "home-2",
            "label": "Corinthians"
        }
    },
    "tab-list.item#home-3": {
        "props": {
            "tabId": "home-3",
            "label": "Palmeiras"
        }
    },
    "tab-list.item#home-4": {
        "props": {
            "tabId": "home-4",
            "label": "Signos"
        }
    },
    "tab-list.item#home-5": {
        "props": {
            "tabId": "home-5",
            "label": "Divertidos"
        }
    },
    "tab-list.item#home-6": {
        "props": {
            "tabId": "home-6",
            "label": "7 Maravilhas"
        }
    },
    "tab-list.item#home-7": {
        "props": {
            "tabId": "home-7",
            "label": "Guerreiros"
        }
    },
    "tab-list.item#home-8": {
        "props": {
            "tabId": "home-8",
            "label": "Álbum 1"
        }
    },
    "tab-list.item#home-9": {
        "props": {
            "tabId": "home-9",
            "label": "Álbum 2"
        }
    },
    "tab-list.item#home-10": {
        "props": {
            "tabId": "home-10",
            "label": "Álbum 3"
        }
    },
    "slider-layout#images": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true,

            "showNavigationArrows": "always",
            "showPaginationDots": "always",
            "blockClass": "carousel"
        },
        "children": ["image#wood-sport", "image#grandes-guerreiros"]
    },
    "image#wood-sport": {
        "props": {
            "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/6iibxhv7jzzdn2749uhoxub2fdjf",
            "maxWidth": "100%",
            "blockClass": "banner"
        }
    },
    "image#grandes-guerreiros": {
        "props": {
            "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/du3k6akoks0u3xe322aypnaaal8e",
            "maxWidth": "100%",
            "blockClass": "banner"
        }
    },
    "slider-layout#controls": {
        "props": {
            // "autoplay": {
            //     "timeout": 7000,
            //     "stopOnHover": true
            // },
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true,
            // "showNavigationArrows": "desktopOnly",
            "blockClass": "carousel"
        },
        "children": ["rich-text#img-1", "rich-text#img-2"]
    },
    "rich-text#img-1": {
        "props": {
            "text": "a"
        }
    },
    "rich-text#img-2": {
        "props": {
            "text": "b"
        }
    },
    "tab-content#home": {
        "children": [
            "tab-content.item#home-1",
